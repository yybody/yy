/* pages/home/home.wxss */
.avatarUrl{
  width: 100%;
  display: flex;
  background-color: #46ce8a;
  height: 250rpx;
}
.avatarUrl image{
        width: 100rpx;
        height: 100rpx;
        margin-top: 75rpx;
        margin-left: 30rpx;
        border: 1rpx solid aliceblue;
        border-radius:50rpx;
}

#nav{
  margin-top: 75rpx;
  display: flex;
  flex-direction: column;
  margin-left:50rpx; 
  }
  

.kb{
  width: 100%;
   height: 30rpx;
   border-bottom:1rpx solid #dadada(0, 0, 0, 0.952); 
  }

.service{
  width: 100%;
  height: 50rpx;
  border-top: 1rpx solid #dadada;
  
  }

  .service view{
  border-right: 1rpx solid #dadada;
  margin-left: 10rpx;
  margin-top: 20rpx;

  }

  #sao,#cre,#lok,#edi,#a1{
  width: 70rpx;
  height: 70rpx;
  padding-top: 10px;
  margin-left: 0rpx;
}

.table{
  width: 100%;
  margin-top: 15rpx;
  border: 1px solid #dadada;
  border-right: 0;
  border-bottom: 0;
}

.tr{
  width: 100%;
  display: flex;
  justify-content: space-between;
  }
  
.th{
  padding: 10px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
  }
  .td{
    padding: 10px;
    border-right: 1px solid #dadada;
    border-bottom: 1px solid #dadada;
    text-align: center;
    width: 19.25%;
  }
.th{
  font-size: 30rpx;
}